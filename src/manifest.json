{
  "manifest_version": 3,
  "name": "Booksmart",
  "description": "Customisable bookmark manager and homepage",
  "version": "0.2",
  "permissions": [
    "bookmarks",
    "downloads",
    "tabs",
    "topSites"
  ],
  "action": {
    "default_icon": "assets/icon.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  }
}