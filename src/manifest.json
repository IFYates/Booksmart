{
  "manifest_version": 3,
  "name": "Booksmart",
  "description": "Customisable start page for quickly accessing your most important bookmarks.",
  "version": "1.3.3",
  "author": "IFYates",
  "icons": {
    "128": "assets/icon.png"
  },
  "permissions": [
    "bookmarks",
    "downloads",
    "storage",
    "tabs",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": "assets/icon.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  }
}